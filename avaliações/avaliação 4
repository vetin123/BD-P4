/* QUESTÃO 01 */
CRIE  TABELA " TB_CURSO " (
  " ID "  INTEIRO  NÃO NULO ,
  " NOME "  TEXTO  NÃO NULO ,
  CHAVE PRIMÁRIA ( " ID " AUTOINCREMENTO)
);

/* QUESTÃO 02 */
CRIE  TABELA " TB_PROFESSOR " (
	" ID "	 INTEIRO  NÃO NULO ,
	" NOME "	 TEXTO  NÃO NULO ,
	" CURSO_ID "	 INTEIRO  NÃO NULO ,
	CHAVE ESTRANGEIRA ( " CURSO_ID " ) REFERÊNCIAS  " TB_CURSO " ( " ID " )
);

/* QUESTÃO 03 */
CRIE  TABELA " TB_CURSO_PROFESSOR " (
	" ID "	 INTEIRO  NÃO NULO ,
	" CURSO_ID "	 INTEIRO  NÃO NULO ,
	" PROFESSOR_ID "	 TEXTO  NÃO NULO ,
	CHAVE ESTRANGEIRA ( " CURSO_ID " ) REFERÊNCIAS  " TB_CURSO " ( " ID " ),
	CHAVE PRIMÁRIA ( " ID " AUTOINCREMENTO)
);

/* QUESTÃO 04 */

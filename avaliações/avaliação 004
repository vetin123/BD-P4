CREATE tabela "TB_CURSO" (
  "ID" INTEIRO NÃO NULO,
  "NOME" TEXTO NÃO NULO,
  chave primária("ID" AUTOINCREMENT)
);

CRIE TABELA "TB_PROFESSOR"(
  "ID" INTEIRO NÃO NULO,
  "NOME" TEXTO NÃO NULO,
  "CURSO_ID" INTEIRO NÃO NULO,
  chave estrangeira("CURSO_ID") REFERÊNCIAS "TB_CURSO"("ID")
);

CRIAR TABELA "TB_CURSO_PROFESSOR" (
  "ID" INTEIRO NÃO NULO,
  "CURSO_ID" INTEIRO NÃO NULO,
  TEXTO "PROFESSOR_ID" NÃO NULO,
  chave estrangeira("CURSO_ID") referencia "TB_CURSO"("ID"),
  chave primária("ID" AUTOINCREMENT)
);

